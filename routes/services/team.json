{
	"title": "Services / Team",
	"description": "Teammates.",
	"sections": [
		
		{
			"id": "list",
			"title": "Teammates",
			"if": "session.profile.services['6486efa055bedc996a215d36'].added",
			"template": "/templates/datatable.html",
			"src": "/api/data?collection=profiles&category=users",
			"columns": [
				{
					"title": "",
					"data": "id",
					"format": "<a href=\"/@{{_id}}\"><div class=\"avatar\" style=\"margin-right: 5px; background-image: url('/cdn/avatars/{{id}}');\"></div><span class=\"handle\" style=\"margin: 5px 0;\">{{id}}</span></a>",
					"searchable": true
				},
				{
					"title": "Name",
					"data": "title",
					"searchable": true
				},
				{
					"title": "Status",
					"data": "mode",
					"searchable": true
				},
				{
					"title": "State",
					"data": "state",
					"searchable": true
				},
				
				{
					"title": "",
					"data": "id",
					"format": "<a class=\"btn btn-easy btn-round text-center\" href=\"/api/access/login/{{_id}}\" target=\"_top\" style=\"margin: 0; padding: 0px; line-height: 55px; font-size: 22px; font-weight: bold;\"><i class=\"fa fa-sign-in\"></i></a>",
					"searchable": false,
					"sortable": false,
					"align": "center"
				}
			]
		}
		
		, {
			"id": "add",
			"if": "session.profile.services['6486efa055bedc996a215d36'].added",
			"template": "/templates/form.html",
			"title": "Add Teammate",
			"background": "#f2f2f2",
			"action": "/api/data?collection=profiles&category=users",
			"method": "POST",
			"reload": true,
			"reset": true,
			"data": {
				
				"title": {
					"required": true,
					"id": "title",
					"title": "Name",
					"type": "text",
					"autocomplete": true,
					"model": false
				}
				
			}
		}
		


		, {
			"title": "Enable",
			"if": "session.uid && !session.profile.services['6486efa055bedc996a215d36'].added",
			"template": "/templates/list.html",
			"data": [
				{
					"title": "<a href=\"/account\">Enable</a> to get started."
				}
			]
		}
		


		, {
			"title": "Access",
			"if": "!session.uid",
			"template": "/templates/list.html",
			"data": [
				{
					"title": "<a href=\"/access/login?redirect=%2Faccount\" target=\"_top\">Login</a> to get started."
				}
			]
		}	

	]
}