{
	"title": "Services / Numbers",
	"description": "Manage numbers.",

	"sections": [
		
		{
			"id": "list",
			"title": "Numbers",
			"if": "session.profile.services['648e4dca55bedc996a231032'].added",
			"template": "/templates/datatable.html",
			"src": "/api/data?collection=profiles&category=numbers",
			"columns": [
				{
					"title": "",
					"data": "id",
					"format": "<a href=\"/@{{_id}}\"><span class=\"handle\">{{id}}</span></a>",
					"searchable": true
				},
				{
					"title": "Target ID",
					"data": "target",
					"format": "<a href=\"/@{{target}}\"><span class=\"handle\">{{target}}</span></a>",
					"searchable": true
				}
			]
		}

		
		
		, {
			"id": "add",
			"if": "session.profile.services['648e4dca55bedc996a231032'].added",
			"template": "/templates/form.html",
			"title": "Add Number",
			"background": "#f2f2f2",
			"action": "/api/data?collection=profiles&category=numbers",
			"method": "POST",
			"reload": true,
			"reset": true,
			"data": {
				
				"id": {
					"required": true,
					"id": "id",
					"title": "Number",
					"type": "text",
					"autocomplete": true,
					"model": false,
					"help": "E. 164 (no spaces): +18886106116"
				}
				
				, "target": {
					"required": false,
					"id": "target",
					"title": "Target ID",
					"type": "text",
					"autocomplete": true,
					"model": false,
					"help": "@ID/handle (do not include @)"
				}
				
			}
		}
		
		


		, {
			"title": "Enable",
			"if": "session.uid && !session.profile.services['648e4dca55bedc996a231032'].added",
			"template": "/templates/list.html",
			"data": [
				{
					"title": "<a href=\"/account\">Enable</a> to get started."
				}
			]
		}


		, {
			"title": "Access",
			"if": "!session.uid",
			"template": "/templates/list.html",
			"data": [
				{
					"title": "<a href=\"/access/login?redirect=%2Faccount\" target=\"_top\">Login</a> to get started."
				}
			]
		}	
		
		
	],
	"resources": []
	
}